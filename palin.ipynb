   "source": [
    "def is_palindrome(text):\n",
    "    cleaned = ''.join(c.lower() for c in text if c.isalnum())\n",
    "    return cleaned == cleaned[::-1]\n",
    "\n",
    "print(is_palindrome(\"Madam\"))  # True\n"
   ]
  }
