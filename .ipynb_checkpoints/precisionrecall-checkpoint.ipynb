    "\n",
    "\n",
    "def precision(tp, fp):\n",
    "    '''\n",
    "    Computes the precision score given the number of true positive (tp)\n",
    "    and false positives(fp)\n",
    "    '''\n",
    "    return tp / (tp + fp) if fp + fp > 0 else 0 \n"
   ]
  }
